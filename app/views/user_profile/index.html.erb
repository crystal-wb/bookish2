<h1>Get to know the family</h1>
    <table class='table'>
    <tr>
        <th>Photo</th>
        <th>Username</th>
        <th>About</th>
        <th>Hobbies</th>
    </tr>
    <% @family.each do |user|%>
    <tr>
        <td><img height='100'src= '<%= user.avatar %>'></td>
        <td><%= user.username %></td>
        <td><%= user.about %></td>
        <td><%= user.hobbies %>
        <a href="/user_profile/<%=user.id%>">Read more</a></td>
    </tr>
    <% end %>
</table>